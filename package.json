{
  "name": "tech-career-growth",
  "description": "A web application that allows developers to find useful resources for their growth in tech",
  "version": "1.0.0",
  "private": false,
  "author": "Eric Cabrel TIOGO <tericcabrel@yahoo.com> (@tericcabrel)",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "prisma",
    "typescript",
    "mysql"
  ],
  "scripts": {
    "dev": "next -p 5800",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "db:dev": "pscale connect tech-career-growth dev --port 3308",
    "db:shadow": "pscale connect tech-career-growth shadow --port 3309",
    "db:deploy:create": "pscale deploy-request create tech-career-growth dev",
    "db:generate": "prisma generate",
    "db:migrate:dev": "prisma migrate dev",
    "db:reset:dev": "prisma migrate reset --skip-seed",
    "db:reset:dev:seed": "prisma migrate reset",
    "db:view": "prisma studio",
    "db:format": "prisma format",
    "db:seed": "prisma db seed",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --cache",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "commit": "cz",
    "type:check": "tsc",
    "postbuild": "next-sitemap"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "lint-staged": {
    "./webapp/src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix --cache",
      "yarn format"
    ]
  },
  "dependencies": {
    "@headlessui/react": "1.4.2",
    "@hookform/resolvers": "2.8.5",
    "@next-auth/prisma-adapter": "1.0.1",
    "@prisma/client": "3.7.0",
    "@sentry/nextjs": "6.16.1",
    "axios": "0.24.0",
    "bcryptjs": "2.4.3",
    "classnames": "2.3.1",
    "dayjs": "1.10.7",
    "gray-matter": "^4.0.3",
    "lodash": "4.17.21",
    "nanoid": "3.1.30",
    "next": "12.0.8",
    "next-auth": "4.0.6",
    "next-connect": "0.11.0",
    "next-seo": "4.28.1",
    "node-emoji": "^1.11.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.22.4",
    "react-hotkeys-hook": "3.4.4",
    "react-markdown": "^8.0.0",
    "react-query": "3.34.6",
    "react-select": "5.2.1",
    "react-toastify": "8.1.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-external-links": "^1.0.1",
    "rehype-raw": "^6.1.1",
    "rehype-slug": "^5.0.1",
    "remark": "^14.0.2",
    "remark-gfm": "^3.0.1",
    "remark-html": "^15.0.1",
    "remark-prism": "^1.3.6",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@tailwindcss/forms": "0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "@testing-library/dom": "8.11.1",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/bcryptjs": "2.4.2",
    "@types/lodash": "4.14.178",
    "@types/node": "17.0.4",
    "@types/node-emoji": "^1.8.1",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "@types/remark-prism": "^1.3.0",
    "@typescript-eslint/eslint-plugin": "5.7.0",
    "@typescript-eslint/parser": "5.7.0",
    "autoprefixer": "10.4.0",
    "babel-jest": "27.4.5",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.5.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-jest-dom": "3.9.2",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-testing-library": "5.0.1",
    "husky": "7.0.4",
    "jest": "27.4.5",
    "lint-staged": "12.1.3",
    "next-sitemap": "^1.9.12",
    "postcss": "8.4.5",
    "prettier": "2.5.1",
    "prisma": "3.7.0",
    "tailwindcss": "3.0.17",
    "ts-node": "10.4.0",
    "typescript": "4.5.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
